projection;
strict ( 1 );
use draft;

define behavior for ZC_JARAP_CERTIFPRODUCT alias Certificate
{
  use create;
  use update;
  use delete;

  use association _Stats { create; with draft; }

  use action Edit;
  use action Activate;
  use action Discard;
  use action Prepare;
  use action Resume;

  use action NewVersion;
  use action ActiveVersion;
  use action InactiveVersion;

}

define behavior for ZC_JARAP_CERTIFSTPRODUCT alias CertificateST
{
  use delete;

  use association _Certif;
}
